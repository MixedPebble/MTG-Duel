{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "typedef": [
          true,
          "property-declaration",
          "variable-declaration",
          "parameter",
          "arrow-parameter",
          "call-signature",
          "arrow-call-signature"
        ]
    }
}
